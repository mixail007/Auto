  <data name="sqlDataSource1.SelectCommand" xml:space="preserve">
    <value>SELECT        Заказ.id AS Номер, Заказ.Наименование, Заказ.Автор, Контрагент.Наименование AS Контрагент, Организация.Наименование AS Организация, Договор.Наименование AS Договор, 
                         Авто.Наименование AS Автомобиль, Авто.VIN AS VIN,Авто.ГодВыпуска AS ГодВыпуска ,Авто.НомерДвигателя AS НомерДвигателя, Цвет.Наименование as Цвет,
 Заказ.СуммаДокумента, ТипЦены.Наименование AS ТипЦены, dbo.ТипЗаказа.Наименование AS ТипЗаказа, dbo.Валюты.Наименование AS Валюта, 
                         Заказ.СрокПоставки, Заказ.СрокСнятияРезерва, Заказ.ДатаСоздания, Заказ.Менеджер
FROM            dbo.ЗаказПокупателя AS Заказ  LEFT OUTER JOIN
                         dbo.ДоговорыВзаиморасчетов AS Договор ON Заказ.ДоговорВзаиморасчетов = Договор.id LEFT OUTER JOIN
                         dbo.Контрагенты AS Контрагент ON Заказ.Контрагент = Контрагент.id LEFT OUTER JOIN
                         dbo.Контрагенты AS Организация ON Заказ.Организация = Контрагент.id LEFT OUTER JOIN
                         dbo.Автомобили AS Авто ON Заказ.Автомобиль = Авто.id LEFT OUTER JOIN
                         dbo.ТипыЦен AS ТипЦены ON Заказ.ТипЦен = ТипЦены.id LEFT OUTER JOIN
                         dbo.ТипЗаказа ON Заказ.ТипЗаказа = dbo.ТипЗаказа.id LEFT OUTER JOIN
                         dbo.Валюты ON Заказ.ВалютаДокумента = dbo.Валюты.id  inner join 
			dbo.Цвет as Цвет on  Авто.Цвет = Цвет.id</value>
  </data>
